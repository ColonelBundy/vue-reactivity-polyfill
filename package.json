{
  "name": "vue-reactivity-with-polyfill",
  "version": "0.1.0",
  "description": "Vue 3 reactivity with polyfill",
  "main": "dist/reactivity.cjs.js",
  "module": "dist/reactivity.esm-bundler.js",
  "jsdelivr": "dist/reactivity.global.js",
  "unpkg": "dist/reactivity.global.js",
  "types": "types/index.d.ts",
  "sideEffects": [
    "src/array.ts",
    "src/collections.ts",
    "src/proxy.ts"
  ],
  "scripts": {
    "test": "jest --runInBand",
    "build": "rm -rf dist && rollup -c rollup.base.config.js && rollup -c rollup.browser.config.js",
    "patch-all": "patch-package core-js && patch-package proxy-polyfill",
    "postinstall": "patch-package",
    "prepublish": "npm run build"
  },
  "author": "dolymood",
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "types"
  ],
  "dependencies": {
    "patch-package": "^6.2.2",
    "proxy-polyfill": "^0.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/jest": "^26.0.9",
    "core-js": "^3.6.5",
    "jest": "^26.3.0",
    "rollup": "^2.23.0",
    "rollup-plugin-terser": "^6.1.0",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7",
    "vue": "^3.0.0-rc.5"
  },
  "peerDependencies": {
    "@vue/reactivity": "^3",
    "@babel/core": "*",
    "@babel/runtime": "^7",
    "core-js": "^3.6.5"
  },
  "browserslist": [
    "> 1%",
    "not ie <= 9",
    "Android >= 4.0",
    "iOS >= 8"
  ]
}
